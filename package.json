{
  "name": "stark-client",
  "version": "0.1.0",
  "description": "Browser Client for the Stark distributed orchestrator.",
  "author": "Adrian Burlacu <adrian.burlacu@live.com>",
  "repository": "https://github.com/WeWatchWall/stark-client",
  "private": true,
  "license": "MIT",
  "keywords": [
    "typescript",
    "esbuild"
  ],
  "browser": "./browser.js",
  "scripts": {
    "build:dev": "esbuild src/browser.ts --bundle --outfile=dist/browser.js",
    "build:watch": "esbuild src/browser.ts --bundle --watch --outfile=dist/browser.js",
    "postinstall": "node build.js"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@types/node": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "esbuild": "^0.11.11",
    "eslint": "^7.22.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.35",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "bootstrap": "^5.1.0",
    "bootstrap-vue": "^2.21.2",
    "esbuild-vue": "^0.3.3",
    "font-awesome": "^4.7.0",
    "vue": "^2.6.14",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.6.14"
  }
}
